{
  "name": "openmrs-contrib-addonindex-ui",
  "description": "UI for the OpenMRS Add-On Index",
  "private": true,
  "scripts": {
    "build:watch": "parcel watch --out-dir ../../../target/classes/static --cache-dir ../../../target/classes/parcel-cache index.html",
    "build:dev": "NODE_ENV=development parcel build --out-dir ../../../target/classes/static --cache-dir ../../../target/classes/parcel-cache --no-minify index.html",
    "build:prod": "parcel build --out-dir ../../../target/classes/static --cache-dir ../../../target/classes/parcel-cache index.html",
    "lint": "eslint --ext js,jsx,ts,tsx",
    "prettier": "prettier app/**/* --write",
    "test": "jest --passWithNoTests"
  },
  "engines": {
    "node": ">=10.23.2"
  },
  "keywords": [
    "OpenMRS"
  ],
  "author": "OpenMRS, Inc",
  "license": "MPL-2.0",
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "ignoreAssets": [
    ".ico"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "bootstrap": "^4.6.0",
    "dayjs": "^1.10.6",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-ga": "^3.3.0",
    "react-query": "^3.18.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-select": "^4.0.17",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "browserslist-config-openmrs": "^1.0.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "jest-fetch-mock": "^3.0.3",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-ignore-assets": "^1.0.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.35.2",
    "sass-loader": "^11.1.1",
    "typescript": "^4.3.5"
  }
}
