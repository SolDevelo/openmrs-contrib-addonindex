{
  "name": "openmrs-contrib-addonindex-ui",
  "description": "UI for the OpenMRS Add-On Index",
  "private": true,
  "scripts": {
    "build:watch": "parcel watch --dist-dir ../../../target/classes/static --cache-dir ../../../target/parcel-cache index.html",
    "build:dev": "NODE_ENV=development parcel build --dist-dir ../../../target/classes/static --cache-dir ../../../target/parcel-cache --no-optimize index.html",
    "build:prod": "parcel build --dist-dir ../../../target/classes/static --cache-dir ../../../target/parcel-cache index.html",
    "lint": "eslint --ext js,jsx,ts,tsx",
    "prettier": "prettier app/**/* --write",
    "test": "jest --passWithNoTests"
  },
  "engines": {
    "node": ">=10.23.2"
  },
  "keywords": [
    "OpenMRS"
  ],
  "author": "OpenMRS, Inc",
  "license": "MPL-2.0",
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "ignoreAssets": [
    ".ico"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "bootstrap": "^4.6.1",
    "dayjs": "^1.11.2",
    "react": "^18.1.0",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.3",
    "react-ga": "^3.3.0",
    "react-query": "^3.39.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.3.2"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/jest": "^26.0.24",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "browserslist-config-openmrs": "^1.0.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^28.1.0",
    "jest-fetch-mock": "^3.0.3",
    "parcel": "^2.5.0",
    "parcel-resolver-ignore": "^2.1.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^18.1.0",
    "sass": "^1.51.0",
    "typescript": "^4.6.4"
  },
  "parcelIgnore": [
    "/favicon.ico",
    "https://id.openmrs.org/.*"
  ]
}
